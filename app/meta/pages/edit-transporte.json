{
  "components": [
    {
      "type": "content",
      "container": "main",
      "components": [
        {
          "type": "row",
          "components": [
            {
              "type": "col",
              "class": "col-md-12",
              "components": [
                {
                  "type": "template-form",
                  "name": "form0",
                  "components": [
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Idt",
                              "component": "idt",
                              "name": "idtLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "int",
                              "type": "numeric",
                              "name": "idt",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${transporte.idt}"
                            },
                            {
                              "type": "required-validator",
                              "name": "idtRequiredValidator",
                              "component": "idt",
                              "text": "idt is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Vium",
                              "component": "idv",
                              "name": "idvLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "data": "${getViaResult}",
                              "textProperty": "idv",
                              "valueProperty": "idv",
                              "placeholder": "Choose Vium",
                              "valueType": "int?",
                              "type": "dropdown",
                              "name": "idv",
                              "style": {
                                "width": "100%"
                              },
                              "value": "${transporte.idv}"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Bilhetes",
                              "component": "bilhetes",
                              "name": "bilhetesLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "type": "textbox",
                              "name": "bilhetes",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${transporte.bilhetes}"
                            },
                            {
                              "type": "required-validator",
                              "name": "bilhetesRequiredValidator",
                              "component": "bilhetes",
                              "text": "bilhetes is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "components": [
                        {
                          "type": "col",
                          "class": "col offset-sm-3",
                          "components": [
                            {
                              "type": "button",
                              "buttonType": "submit",
                              "icon": "save",
                              "text": "Save",
                              "buttonStyle": "primary"
                            },
                            {
                              "type": "button",
                              "buttonStyle": "light",
                              "style": {
                                "marginLeft": "1rem"
                              },
                              "text": "Cancel",
                              "events": {
                                "click": [
                                  {
                                    "type": "close-popup"
                                  }
                                ]
                              }
                            }
                          ]
                        }
                      ]
                    }
                  ],
                  "events": {
                    "submit": [
                      {
                        "error": [
                          {
                            "detail": "Unable to update Transporte",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "find_supermarket_db.updateTransporte",
                        "parameters": [
                          {
                            "name": "idt",
                            "value": "${parameters.idt}"
                          },
                          {
                            "name": "Transporte",
                            "value": "${transporte}"
                          }
                        ],
                        "then": [
                          {
                            "type": "close-popup",
                            "result": "${transporte}",
                            "condition": ""
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "itemType": "FindSupermarket.Models.FindSupermarketDb.Transporte",
                  "data": "${transporte}",
                  "visible": "${transporte != null}"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "events": {
    "load": [
      {
        "name": "find_supermarket_db.getTransporteByidt",
        "parameters": [
          {
            "name": "idt",
            "value": "${parameters.idt}"
          }
        ],
        "then": [
          {
            "name": "transporte",
            "type": "set",
            "value": "${result}",
            "valueType": "FindSupermarket.Models.FindSupermarketDb.Transporte"
          }
        ],
        "error": [],
        "type": "invoke"
      },
      {
        "name": "find_supermarket_db.getVia",
        "parameters": [],
        "then": [
          {
            "name": "getViaResult",
            "type": "set",
            "value": "${result}",
            "valueType": "IEnumerable<FindSupermarket.Models.FindSupermarketDb.Vium>"
          }
        ],
        "type": "invoke"
      }
    ]
  },
  "name": "Edit Transporte",
  "layout": "Main"
}