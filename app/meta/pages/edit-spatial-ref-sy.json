{
  "components": [
    {
      "type": "content",
      "container": "main",
      "components": [
        {
          "type": "row",
          "components": [
            {
              "type": "col",
              "class": "col-md-12",
              "components": [
                {
                  "type": "template-form",
                  "name": "form0",
                  "components": [
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Auth Srid",
                              "component": "auth_srid",
                              "name": "auth_sridLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "int?",
                              "type": "numeric",
                              "name": "auth_srid",
                              "style": {
                                "width": "100%"
                              },
                              "value": "${spatialrefsy.auth_srid}"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Srtext",
                              "component": "srtext",
                              "name": "srtextLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "maxLength": 2048,
                              "type": "textbox",
                              "name": "srtext",
                              "style": {
                                "width": "100%"
                              },
                              "value": "${spatialrefsy.srtext}"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Srid",
                              "component": "srid",
                              "name": "sridLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "int",
                              "type": "numeric",
                              "name": "srid",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${spatialrefsy.srid}"
                            },
                            {
                              "type": "required-validator",
                              "name": "sridRequiredValidator",
                              "component": "srid",
                              "text": "srid is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Proj 4 Text",
                              "component": "proj4text",
                              "name": "proj4textLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "maxLength": 2048,
                              "type": "textbox",
                              "name": "proj4text",
                              "style": {
                                "width": "100%"
                              },
                              "value": "${spatialrefsy.proj4text}"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Auth Name",
                              "component": "auth_name",
                              "name": "auth_nameLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "maxLength": 256,
                              "type": "textbox",
                              "name": "auth_name",
                              "style": {
                                "width": "100%"
                              },
                              "value": "${spatialrefsy.auth_name}"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "components": [
                        {
                          "type": "col",
                          "class": "col offset-sm-3",
                          "components": [
                            {
                              "type": "button",
                              "buttonType": "submit",
                              "icon": "save",
                              "text": "Save",
                              "buttonStyle": "primary"
                            },
                            {
                              "type": "button",
                              "buttonStyle": "light",
                              "style": {
                                "marginLeft": "1rem"
                              },
                              "text": "Cancel",
                              "events": {
                                "click": [
                                  {
                                    "type": "close-popup"
                                  }
                                ]
                              }
                            }
                          ]
                        }
                      ]
                    }
                  ],
                  "events": {
                    "submit": [
                      {
                        "error": [
                          {
                            "detail": "Unable to update SpatialRefSy",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "find_supermarket_db.updateSpatialRefSy",
                        "parameters": [
                          {
                            "name": "srid",
                            "value": "${parameters.srid}"
                          },
                          {
                            "name": "SpatialRefSy",
                            "value": "${spatialrefsy}"
                          }
                        ],
                        "then": [
                          {
                            "type": "close-popup",
                            "result": "${spatialrefsy}",
                            "condition": ""
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "itemType": "FindSupermarket.Models.FindSupermarketDb.SpatialRefSy",
                  "data": "${spatialrefsy}",
                  "visible": "${spatialrefsy != null}"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "events": {
    "load": [
      {
        "name": "find_supermarket_db.getSpatialRefSyBysrid",
        "parameters": [
          {
            "name": "srid",
            "value": "${parameters.srid}"
          }
        ],
        "then": [
          {
            "name": "spatialrefsy",
            "type": "set",
            "value": "${result}",
            "valueType": "FindSupermarket.Models.FindSupermarketDb.SpatialRefSy"
          }
        ],
        "error": [],
        "type": "invoke"
      }
    ]
  },
  "name": "Edit Spatial Ref Sy",
  "layout": "Main"
}