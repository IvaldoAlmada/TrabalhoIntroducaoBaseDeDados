{
  "components": [
    {
      "type": "content",
      "container": "main",
      "components": [
        {
          "type": "heading",
          "name": "pageTitle",
          "size": "H1",
          "text": "Transportes"
        },
        {
          "type": "row",
          "components": [
            {
              "type": "col",
              "class": "col-md-12",
              "components": [
                {
                  "icon": "add_circle_outline",
                  "name": "button0",
                  "style": {
                    "marginBottom": "10px"
                  },
                  "text": "Add",
                  "type": "button",
                  "events": {
                    "click": [
                      {
                        "path": "add-transporte",
                        "type": "popup",
                        "then": [
                          {
                            "code": "await grid0.Reload();",
                            "type": "execute"
                          },
                          {
                            "code": "await InvokeAsync(() => { StateHasChanged(); });",
                            "type": "execute"
                          }
                        ]
                      }
                    ]
                  }
                },
                {
                  "name": "grid0",
                  "allowFiltering": true,
                  "filterMode": "advanced",
                  "allowPaging": true,
                  "allowSorting": true,
                  "data": "${getTransportesResult}",
                  "events": {
                    "loadData": [],
                    "rowSelect": [
                      {
                        "parameters": [
                          {
                            "name": "idt",
                            "value": "${event.idt}"
                          }
                        ],
                        "path": "edit-transporte",
                        "type": "popup",
                        "then": [
                          {
                            "code": "await InvokeAsync(() => { StateHasChanged(); });",
                            "type": "execute"
                          }
                        ]
                      }
                    ]
                  },
                  "title": "",
                  "columns": [
                    {
                      "property": "idt",
                      "title": "Idt"
                    },
                    {
                      "property": "idv",
                      "sortProperty": "Vium.idv",
                      "groupProperty": "Vium.idv",
                      "filterProperty": "Vium.idv",
                      "template": "${data.Vium?.idv}",
                      "title": "Vium"
                    },
                    {
                      "property": "bilhetes",
                      "title": "Bilhetes"
                    },
                    {
                      "filterable": false,
                      "sortable": false,
                      "width": "70px",
                      "textAlign": "center",
                      "template": [
                        {
                          "attributes": [
                            {
                              "name": "@onclick:stopPropagation",
                              "value": true
                            }
                          ],
                          "buttonStyle": "danger",
                          "icon": "close",
                          "name": "gridDeleteButton",
                          "text": "",
                          "size": "sm",
                          "type": "button",
                          "events": {
                            "click": [
                              {
                                "condition": "await DialogService.Confirm(\"Are you sure you want to delete this record?\") == true",
                                "error": [
                                  {
                                    "detail": "Unable to delete Transporte",
                                    "severity": "error",
                                    "summary": "Error",
                                    "type": "notify"
                                  }
                                ],
                                "name": "find_supermarket_db.deleteTransporte",
                                "parameters": [
                                  {
                                    "name": "idt",
                                    "value": "${data.idt}"
                                  }
                                ],
                                "then": [
                                  {
                                    "code": "await grid0.Reload();",
                                    "condition": "result != null",
                                    "type": "execute"
                                  }
                                ],
                                "type": "invoke"
                              }
                            ]
                          }
                        }
                      ]
                    }
                  ],
                  "type": "datagrid",
                  "itemType": "FindSupermarket.Models.FindSupermarketDb.Transporte"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "events": {
    "load": [
      {
        "name": "find_supermarket_db.getTransportes",
        "parameters": [
          {
            "name": "$expand",
            "value": "Vium"
          }
        ],
        "then": [
          {
            "name": "getTransportesResult",
            "type": "set",
            "value": "${result}",
            "valueType": "IEnumerable<FindSupermarket.Models.FindSupermarketDb.Transporte>"
          }
        ],
        "type": "invoke"
      }
    ]
  },
  "name": "Transportes",
  "layout": "Main"
}